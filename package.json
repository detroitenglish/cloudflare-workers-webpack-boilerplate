{
  "name": "cloudflare-worker-webpack-boilerplate",
  "description": "A superbly simple, minimal-config template to build, bundle and deploy Cloudflare Workers with Webpack.",
  "author": "Dave Willenberg <dave@detroit-english.de> (https://github.com/detroitenglish)",
  "version": "2.2.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/detroitenglish/cloudflare-worker-webpack-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/detroitenglish/cloudflare-worker-webpack-boilerplate/issues"
  },
  "homepage": "https://github.com/detroitenglish/cloudflare-worker-webpack-boilerplate",
  "scripts": {
    "build": "webpack --bail --no-deploy",
    "build:example": "webpack --bail --example --no-deploy",
    "deploy": "webpack --bail --deploy",
    "deploy:example": "webpack --bail --example --deploy",
    "lint": "eslint src lib test webpack.config.js --ignore-pattern=eslint.test.js",
    "reset": "webpack --bail --reset",
    "test": "ava --fail-fast"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "core-js": "^3.1.2",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "yargs": "^13.2.4"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^2.0.2",
    "cloudflare-worker-webpack-plugin": "^2.0.3",
    "colors": "^1.3.3",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "lodash.sample": "^4.2.1",
    "mkdirp": "^0.5.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "engines": {
    "node": ">= 8"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm-run-all -p lint test"
    }
  }
}
